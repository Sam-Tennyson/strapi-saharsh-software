{
  "kind": "collectionType",
  "collectionName": "news_posts",
  "info": {
    "singularName": "news-post",
    "pluralName": "news-posts",
    "displayName": "NewsPost",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "link": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "imageURL": {
      "type": "string"
    },
    "publicationDate": {
      "type": "date"
    },
    "keywords": {
      "type": "text"
    },
    "source": {
      "type": "string"
    },
    "shortenedBy": {
      "type": "string",
      "default": "Stavros Lee"
    },
    "categories": {
      "type": "text"
    },
    "locations": {
      "type": "text"
    },
    "shortenedByPhotoURL": {
      "type": "string"
    },
    "groupId": {
      "type": "string",
      "default": "Puerto Rico",
      "required": true
    },
    "originalContent": {
      "type": "text",
      "private": true
    },
    "isDuplicate": {
      "type": "boolean",
      "default": false
    },
    "duplicatedBy": {
      "type": "integer",
      "private": true
    },
    "enSlug": {
      "type": "uid",
      "required": true,
      "targetField": "enTitle"
    },
    "entities": {
      "type": "text",
      "private": true
    },
    "embeddings": {
      "type": "json",
      "private": true
    },
    "language": {
      "type": "string"
    },
    "photoURL": {
      "type": "string"
    },
    "smallPhotoURL": {
      "type": "string"
    },
    "enTitle": {
      "type": "string"
    },
    "enSummary": {
      "type": "text"
    },
    "esTitle": {
      "type": "string"
    },
    "esSummary": {
      "type": "text"
    },
    "esSlug": {
      "type": "uid",
      "targetField": "esTitle"
    }
  }
}
